
@model IndexViewModel

@{
    ViewData["Title"] = "Home Page";
}

<!--  category Section Start -->
<!--  For developer (More icons find in https://www.svgrepo.com/) -->
<section class="section ec-category-section section-space-p">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="section-title">
                    <h2 class="ec-title">دسته‌بندی‌های برتر</h2>
                </div>
            </div>
        </div>

         <div class="row mt-1 margin-minus-b-15 margin-minus-t-15">
            <div id="ec-cat-slider" class="ec-cat-slider owl-carousel">
                        @foreach (var item in Model.ParentCategories)
                        {
                <div class="ec_cat_content ec_cat_content_1">
                    <div class="ec_cat_inner ec_cat_inner-1">

                            <div>
                                <img src="~/Upload/Img/Category/@item.PictureFileName"
                                 class="svg_img"
                                     alt=@item.Title height="50px" width="70px" />
                             </div>
                            <div class="ec-category-desc">
                                <a href="shop-left-sidebar-col-3.html" class="cat-show-all">
                                <h3> @item.Title <span title="Category Items">(53)</span></h3>
                                </a>                                   
                            </div>
                        
                        </div>
                </div>
                }
             </div>
        </div>

    </div>
</section>
<!--category Section End -->
<!--  لیست غرفه ها Start -->
<section class="section ec-blog-section section-space-p">
    <div class="container">
        <div class="col-md-12">
            <div class="section-title">
                <h2 class="ec-title">غرفه ها</h2>
            </div>
        </div>

        <div class="row">
            <div class="ec-blog-slider owl-carousel" data-animation="fadeIn">
                @foreach (var item in Model.boothOutputs)
                {
                <div class="ec-blog-block">
                    <div class="ec-blog-inner">
                        <div class="ec-blog-image">
                            <a href="blog-detail-left-sidebar.html">
                                <img class="blog-image"
                                         src="~/Upload/Img/BoothAvatar/@item.AvatarPictureFile"
                                     alt="Blog"  height="180px" />
                            </a>
                        </div>
                        <div class="ec-blog-content">
                            <h5 class="ec-blog-title">
                                <a href="blog-detail-left-sidebar.html">
                                   @item.Name
                                </a>
                            </h5>

                            <div class="ec-blog-date">
                                    @item.Description
                            </div>
                        </div>
                    </div>
                </div>
                }
            </div>
        </div>
    </div>


</section>
<!--  لیست غرفه ها End -->
<!-- Product tab Area Start -->
