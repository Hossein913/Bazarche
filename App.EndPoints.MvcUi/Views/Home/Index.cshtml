                        <h2 class="ec-title">پیشنهاد ویژه </h2>
                    </div>
                </div>

                <div class="ec-spe-products">
                    @foreach (var item in Model.auctionOutputs)
                    {
                        
                        <!---  مزایده اول --->
                        <div class="ec-spe-product">
                            <div class="ec-spe-pro-inner">
                                <div class="ec-spe-pro-image-outer col-md-6 col-sm-12">
                                    <div class="ec-spe-pro-image">
                                        <img class="img-responsive"
                                             src="~/Upload/Img/Product/@item.ProductDto.Avatar"
                                             alt="" />
                                    </div>
                                </div>
                                <div class="ec-spe-pro-content col-md-6 col-sm-12 mx-1">
                                    <div class="ec-spe-pro-rating">
                                        <span class="ec-spe-rating-icon">
                                            <i class="ecicon eci-star fill"></i>
                                            <i class="ecicon eci-star fill"></i>
                                            <i class="ecicon eci-star fill"></i>
                                            <i class="ecicon eci-star"></i>
                                            <i class="ecicon eci-star"></i>
                                        </span>
                                    </div>
                                    <h5 class="ec-spe-pro-title">
                                        <a href="product-left-sidebar.html">
                                            @item.ProductDto.Name
                                        </a>
                                    </h5>
                                    <div class="ec-spe-pro-desc">
                                            @item.ProductDto.Description
                                    </div>
                                    @if (item.Bids.Count > 0)
                                    {
                                        <div class="ec-spe-price">
                                            <span class="new-price">بالاتربین پیشنهاد  @item.Bids.Max(b => b.BidPrice)</span>

                                        </div>
                                    }
                                    else
                                    {
                                       <div class="ec-spe-price">
                                            <span class="new-price">اولین پیشنهاد را شما دهید</span>

                                        </div>
                                        
                                    }

                                    <div class="ec-spe-pro-btn">
                                        <a href="#" class="btn btn-lg btn-primary">
                                            ارسال پیشنهاد
                                        </a>
                                    </div>
                                    <div class="ec-spe-pro-progress">
                                        <span class="ec-spe-pro-progress-desc">
                                            <span> فروخته شده: <b>15</b></span><span>موجودی: <b>40</b></span>
                                        </span>
                                        <span class="ec-spe-pro-progressbar"></span>
                                    </div>
                                    <div class="countdowntimer">
                                        <span class="ec-spe-count-desc">
                                            عجله کن! پیشنهاد به پایان می رسد در:
                                        </span>
                                        <span id="ec-spe-count-1"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!---  مزایده اول --->
